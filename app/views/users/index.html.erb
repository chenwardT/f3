<%= paginate @users %>

<div id="user-list">
  <table class="table">
    <thead>
      <tr>
        <th>Members</th>
        <th>Posts</th>
        <th>Join Date</th>
      </tr>
    </thead>
    <% @users.each do |user| %>
      <tr>
        <td><a href="<%= user_path(user) %>"><%= user.username %></a></td>
        <td><a href="#"><%= user.posts.count %></a></td>
        <td><%= user.created_at %></td>
      </tr>
    <% end %>
  </table>
</div>

<%= paginate @users %>